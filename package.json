{
  "name": "@magelan/docker-screenshot",
  "version": "1.0.0",
  "description": "Custom puppeteer automation to make screenshot",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "DEBUG='true' nodemon yarn start",
    "test": "yarn run format:check",
    "format:check": "prettier \"./**/*.{js,json,md}\" --check",
    "format:fix": "prettier \"./**/*.{js,json,md}\" --write"
  },
  "dependencies": {
    "data-urls": "^1.1.0",
    "koa": "^2.7.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "puppeteer": "^1.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0"
  }
}
